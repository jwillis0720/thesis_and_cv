number = {10-11},
pages = {857--883},
month = feb,
affiliation = {Laboratoire d'ImmunoG{\'e}n{\'e}tique Mol{\'e}culaire (LIGM), Universit{\'e} Montpellier II, UPR CNRS 1142, IGH, 141 rue de la Cardonille, 34396 Montpellier Cedex 5, France.},
