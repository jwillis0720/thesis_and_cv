@article{Koch:2003we,
author = {Koch, Markus and Pancera, Marie and Kwong, Peter D and Kolchinsky, Peter and Grundner, Christoph and Wang, Liping and Hendrickson, Wayne A and Sodroski, Joseph and Wyatt, Richard},
title = {{Structure-based, targeted deglycosylation of HIV-1 gp120 and effects on neutralization sensitivity and antibody recognition.}},
journal = {Virology},
year = {2003},
volume = {313},
number = {2},
pages = {387--400},
month = sep,
affiliation = {Department of Cancer Immunology and AIDS, Dana-Farber Cancer Institute, Boston, MA 02115, USA.},
pmid = {12954207},
language = {English},
rating = {0},
date-added = {2009-10-09T23:51:25GMT},
date-modified = {2014-03-17T02:35:28GMT},
abstract = {The human immunodeficiency virus (HIV-1) exterior envelope glycoprotein, gp120, mediates receptor binding and is the major target for neutralizing antibodies. Primary HIV-1 isolates are characteristically more resistant to broadly neutralizing antibodies, although the structural basis for this resistance remains obscure. Most broadly neutralizing antibodies are directed against functionally conserved gp120 regions involved in binding to either the primary virus receptor, CD4, or the viral coreceptor molecules that normally function as chemokine receptors. These antibodies are known as CD4 binding site (CD4BS) and CD4-induced (CD4i) antibodies, respectively. Inspection of the gp120 crystal structure reveals that although the receptor-binding regions lack glycosylation, sugar moieties lie proximal to both receptor-binding sites on gp120 and thus in proximity to both the CD4BS and the CD4i epitopes. In this study, guided by the X-ray crystal structure of gp120, we deleted four N-linked glycosylation sites that flank the receptor-binding regions. We examined the effects of selected changes on the sensitivity of two prototypic HIV-1 primary isolates to neutralization by antibodies. Surprisingly, removal of a single N-linked glycosylation site at the base of the gp120 third variable region (V3 loop) increased the sensitivity of the primary viruses to neutralization by CD4BS antibodies. Envelope glycoprotein oligomers on the cell surface derived from the V3 glycan-deficient virus were better recognized by a CD4BS antibody and a V3 loop antibody than were the wild-type glycoproteins. Absence of all four glycosylation sites rendered a primary isolate sensitive to CD4i antibody-mediated neutralization. Thus, carbohydrates that flank receptor-binding regions on gp120 protect primary HIV-1 isolates from antibody-mediated neutralization.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=12954207&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2003/Koch/Virology%202003%20Koch.pdf},
file = {{Virology 2003 Koch.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2003/Koch/Virology 2003 Koch.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/159FDC0E-E31A-4A0C-B0FC-4BD8074BBAE5}}
}

@article{Meyuhas:2009iv,
author = {Meyuhas, Ronit and Noy, Hava and Fishman, Sigal and Margalit, Alon and Montefiori, David C and Gross, Gideon},
title = {{Enhanced HIV-1 neutralization by a CD4-VH3-IgG1 fusion protein}},
journal = {Biochemical and biophysical research communications},
year = {2009},
volume = {386},
number = {2},
pages = {402--406},
month = aug,
affiliation = {Laboratory of Immunology, MIGAL, Kiryat Shmona 11016, Israel.},
doi = {10.1016/j.bbrc.2009.06.057},
pmid = {19538939},
language = {eng},
rating = {0},
date-added = {2009-10-09T23:53:17GMT},
date-modified = {2014-03-17T02:35:28GMT},
abstract = {HIV-1 gp120 is an alleged B cell superantigen, binding certain VH3+ human antibodies. We reasoned that a CD4-VH3 fusion protein could possess higher affinity for gp120 and improved HIV-1 inhibitory capacity. To test this we produced several human IgG1 immunoligands harboring VH3. Unlike VH3-IgG1 or VH3-CD4-IgG1, CD4-VH3-IgG1 bound gp120 considerably stronger than CD4-IgG1. CD4-VH3-IgG1 exhibited approximately 1.5-2.5-fold increase in neutralization of two T-cell laboratory-adapted strains when compared to CD4-IgG1. CD4-VH3-IgG1 improved neutralization of 7/10 clade B primary isolates or pseudoviruses, exceeding 20-fold for JR-FL and 13-fold for Ba-L. It enhanced neutralization of 4/8 clade C viruses, and had negligible effect on 1/4 clade A pseudoviruses. We attribute this improvement to possible pairing of VH3 with CD4 D1 and stabilization of an Ig Fv-like structure, rather than to superantigen interactions. These novel findings support the current notion that CD4 fusion proteins can act as better HIV-1 entry inhibitors with potential clinical implications.},
url = {http://www.sciencedirect.com/science?_ob=ArticleURL&_udi=B6WBK-4WJ3DPM-3&_user=86629&_rdoc=1&_fmt=&_orig=search&_sort=d&_docanchor=&view=c&_acct=C000006878&_version=1&_urlVersion=0&_userid=86629&md5=4996c7bfadc790b802ff2ff1359202f8},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2009/Meyuhas/Biochem%20Biophys%20Res%20Commun%202009%20Meyuhas.pdf},
file = {{Biochem Biophys Res Commun 2009 Meyuhas.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2009/Meyuhas/Biochem Biophys Res Commun 2009 Meyuhas.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.bbrc.2009.06.057}}
}

@article{Montefiori:2009hj,
