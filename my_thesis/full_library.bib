year = {2010},
volume = {132},
number = {44},
pages = {15484--15486},
month = nov,
affiliation = {Laboratory of Chemical Physics, National Institute of Diabetes and Digestive and Kidney Diseases, National Institutes of Health, Bethesda, Maryland 20892, USA. AlexanderG@intra.niddk.nih.gov},
