}

@article{Crouzier:1995ua,
author = {Crouzier, R and Martin, T and Pasquali, J L},
title = {{Heavy chain variable region, light chain variable region, and heavy chain CDR3 influences on the mono- and polyreactivity and on the affinity of human monoclonal rheumatoid factors.}},
journal = {Journal of immunology (Baltimore, Md : 1950)},
year = {1995},
volume = {154},
number = {9},
pages = {4526--4535},
month = may,
annote = {Germline primary sequence and CD3 are responsible for autoantibodies. This is a nice first step in the justification that some germline repertoires may be responsible for an entire subset of pathology
},
affiliation = {Laboratory of Immunopathology, University Hospitals of Strasbourg, France.},
keywords = {PNAS_Paper},
pmid = {7722307},
language = {English},
read = {Yes},
