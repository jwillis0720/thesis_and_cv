journal = {Journal of computer-aided molecular design},
year = {2001},
volume = {15},
number = {5},
pages = {411--428},
month = may,
affiliation = {Department of Pharmaceutical Chemistry, School of Pharmacy, University of California, San Francisco 94143-0446, USA.},
