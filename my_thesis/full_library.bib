uri = {\url{papers2://publication/doi/10.1007/s00894-005-0054-2}}
}

@article{Hammonds:2007ht,
author = {Hammonds, Jason and Chen, Xuemin and Zhang, Xiugen and Lee, Francis and Spearman, Paul},
