file = {{Cytokine 2008 Kenny.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2008/Kenny/Cytokine 2008 Kenny.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.cyto.2008.07.010}}
}

@article{Oliveira:2008eo,
author = {Oliveira, Sergio Costa and de Oliveira, Fernanda Souza and Macedo, Gilson Costa and de Almeida, Leonardo Augusto and Carvalho, Natalia Barbosa},
title = {{The role of innate immune receptors in the control of Brucella abortus infection: toll-like receptors and beyond}},
journal = {Microbes and infection / Institut Pasteur},
year = {2008},
volume = {10},
number = {9},
pages = {1005--1009},
month = jul,
affiliation = {Department of Biochemistry and Immunology, Federal University of Minas Gerais, Pampulha, 31270-901 Belo Horizonte, MG, Brazil. scozeus@icb.ufmg.br},
doi = {10.1016/j.micinf.2008.07.005},
pmid = {18664388},
language = {eng},
read = {Yes},
