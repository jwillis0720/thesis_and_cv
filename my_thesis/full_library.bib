pages = {2987--2998},
month = apr,
affiliation = {Department of Chemistry, Vanderbilt University, 7330 Stevenson Center, Station B 351822, Nashville, Tennessee 37235, USA.},
keywords = {tag},
doi = {10.1021/bi902153g},
