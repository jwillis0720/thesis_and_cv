number = {3},
pages = {342--349},
month = sep,
affiliation = {School of Biochemistry and Immunology, Trinity College, Dublin, Ireland. kennyef@tcd.ie},
doi = {10.1016/j.cyto.2008.07.010},
pmid = {18706831},
language = {eng},
read = {Yes},
