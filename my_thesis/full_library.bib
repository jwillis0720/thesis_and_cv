@article{Baker:2013bg,
author = {Baker, Rachael and Lewis, Steven M and Sasaki, Atsuo T and Wilkerson, Emily M and Locasale, Jason W and Cantley, Lewis C and Kuhlman, Brian and Dohlman, Henrik G and Campbell, Sharon L},
