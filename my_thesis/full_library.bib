@article{Klasse:2013fs,
author = {Klasse, Per Johan and Klasse, Per Johan and Depetris, Rafael S and Depetris, Rafael S and Pejchal, Robert and Pejchal, Robert and Julien, Jean-Philippe and Julien, Jean-Philippe and Khayat, Reza and Khayat, Reza and Lee, Jeong Hyun and Lee, Jeong Hyun and Marozsan, Andre J and Marozsan, Andre J and Cupo, Albert and Cupo, Albert and Cocco, Nicolette and Cocco, Nicolette and Korzun, Jacob and Korzun, Jacob and Yasmeen, Anila and Yasmeen, Anila and Ward, Andrew B and Ward, Andrew B and Wilson, Ian A and Wilson, Ian A and Sanders, Rogier W and Sanders, Rogier W and Moore, John P and Moore, John P},
