}

@article{Gray:2006dy,
author = {Gray, Jeffrey J},
title = {{High-resolution protein-protein docking}},
journal = {Current opinion in structural biology},
year = {2006},
volume = {16},
number = {2},
pages = {183--193},
month = apr,
affiliation = {Department of Chemical {\&} Biomolecular Engineering and Program in Molecular {\&} Computational Biophysics, Johns Hopkins University, 3400 North Charles Street, Baltimore, MD 21218, USA. jgray@jhu.edu},
doi = {10.1016/j.sbi.2006.03.003},
