local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2013/Pancera/Nat.%20Struct.%20Mol.%20Biol.%202013%20Pancera-2.pdf},
file = {{Nat. Struct. Mol. Biol. 2013 Pancera-2.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2013/Pancera/Nat. Struct. Mol. Biol. 2013 Pancera-2.pdf:application/pdf;Nat. Struct. Mol. Biol. 2013 Pancera-2.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2013/Pancera/Nat. Struct. Mol. Biol. 2013 Pancera-2.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1038/nsmb.2600}}
}

