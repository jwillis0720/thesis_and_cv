@article{Klein:2013iz,
author = {Klein, Florian and Diskin, Ron and Scheid, Johannes F and Gaebler, Christian and Mouquet, Hugo and Georgiev, Ivelin S and Pancera, Marie and Zhou, Tongqing and Incesu, Reha-Baris and Fu, Brooks Zhongzheng and Gnanapragasam, Priyanthi N P and Oliveira, Thiago Y and Seaman, Michael S and Kwong, Peter D and Bjorkman, Pamela J and Nussenzweig, Michel C},
title = {{Somatic mutations of the immunoglobulin framework are generally required for broad and potent HIV-1 neutralization.}},
journal = {Cell},
year = {2013},
volume = {153},
number = {1},
pages = {126--138},
month = mar,
annote = {Awesome section for reviews.

Say that reversion of framework reduces flexibility and thereby breadth. However show that PG16 is an exception to this rule. Awesome.},
affiliation = {Laboratory of Molecular Immunology, The Rockefeller University, New York, NY 10065, USA.},
doi = {10.1016/j.cell.2013.03.018},
pmid = {23540694},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-07-31T08:03:46GMT},
date-modified = {2014-03-17T02:39:07GMT},
abstract = {Broadly neutralizing antibodies (bNAbs) to HIV-1 can prevent infection and are therefore of great importance for HIV-1 vaccine design. Notably, bNAbs are highly somatically mutated and generated by a fraction of HIV-1-infected individuals several years after infection. Antibodies typically accumulate mutations in the complementarity determining region (CDR) loops, which usually contact the antigen. The CDR loops are scaffolded by canonical framework regions (FWRs) that are both resistant to and less tolerant of mutations. Here, we report that in contrast to most antibodies, including those with limited HIV-1 neutralizing activity, most bNAbs require somatic mutations in their FWRs. Structural and functional analyses reveal that somatic mutations in FWR residues enhance breadth and potency by providing increased flexibility and/or direct antigen contact. Thus, in bNAbs, FWRs play an essential role beyond scaffolding the CDR loops and their unusual contribution to potency and breadth should be considered in HIV-1 vaccine design.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=23540694&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2013/Klein/Cell%202013%20Klein-2.pdf},
file = {{Cell 2013 Klein-2.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2013/Klein/Cell 2013 Klein-2.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.cell.2013.03.018}}
}

