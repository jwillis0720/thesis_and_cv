journal = {Cell},
year = {1996},
volume = {85},
number = {1},
pages = {107--113},
month = apr,
affiliation = {Laboratory of Molecular Biology, National Institute of Diabetes and Digestive and Kidney Diseases, National Institutes of Health, Bethesda, Maryland 20892, USA.},
