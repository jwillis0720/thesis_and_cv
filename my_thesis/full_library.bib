file = {{J Virol 2012 Doria-Rose.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2012/Doria-Rose/J Virol 2012 Doria-Rose.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1128/JVI.00696-12}}
}

@article{Dowd:2002ue,
author = {Dowd, Cynthia S and Leavitt, Stephanie and Babcock, Gregory and Godillot, Alexis P and Van Ryk, Don and Canziani, Gabriela A and Sodroski, Joseph and Freire, Ernesto and Chaiken, Irwin M},
title = {{Beta-turn Phe in HIV-1 Env binding site of CD4 and CD4 mimetic miniprotein enhances Env binding affinity but is not required for activation of co-receptor/17b site.}},
journal = {Biochemistry},
year = {2002},
volume = {41},
