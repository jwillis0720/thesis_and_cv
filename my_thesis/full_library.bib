file = {{Annu Rev Med 2009 Douek.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2009/Douek/Annu Rev Med 2009 Douek.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1146/annurev.med.60.041807.123549}}
}

@article{McMichael:2010cg,
