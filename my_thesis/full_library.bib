language = {English},
read = {Yes},
rating = {0},
date-added = {2012-01-14T02:40:03GMT},
date-modified = {2014-03-17T02:36:21GMT},
abstract = {High-resolution structures reveal how a germline antibody can recognize a range of clinically relevant carbohydrate epitopes. The germline response to a carbohydrate immunogen can be critical to survivability, with selection for antibody gene segments that both confer protection against common pathogens and retain the flexibility to adapt to new disease organisms. We show here that antibody S25-2 binds several distinct inner-core epitopes of bacterial lipopolysaccharides (LPSs) by linking an inherited monosaccharide residue binding site with a subset of complementarity-determining regions (CDRs) of limited flexibility positioned to recognize the remainder of an array of different epitopes. This strategy allows germline antibodies to adapt to different epitopes while minimizing entropic penalties associated with the immobilization of labile CDRs upon binding of antigen, and provides insight into the link between the genetic origin of individual CDRs and their respective roles in antigen recognition.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=14625588&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2003/Nguyen/Nat.%20Struct.%20Biol.%202003%20Nguyen.pdf},
file = {{Nat. Struct. Biol. 2003 Nguyen.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2003/Nguyen/Nat. Struct. Biol. 2003 Nguyen.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1038/nsb1014}}
}

@article{Rini:1993uh,
