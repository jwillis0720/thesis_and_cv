title = {{Preconfiguration of the antigen-binding site during affinity maturation of a broadly neutralizing influenza virus antibody.}},
journal = {The king cobra genome reveals dynamic gene evolution and adaptation in the snake venom system},
year = {2013},
volume = {110},
number = {1},
pages = {264--269},
month = jan,
affiliation = {Laboratory of Molecular Medicine, Children's Hospital, Harvard Medical School and Howard Hughes Medical Institute, Boston, MA 02115.},
doi = {10.1073/pnas.1218256109},
