@article{Oettinger:1990ud,
author = {Oettinger, M A MA and Schatz, D G DG and Gorka, C C and Baltimore, D D},
title = {{RAG-1 and RAG-2, adjacent genes that synergistically activate V(D)J recombination.}},
journal = {Science (New York, NY)},
year = {1990},
volume = {248},
number = {4962},
pages = {1517--1523},
month = jun,
affiliation = {Whitehead Institute for Biomedical Research, Cambridge, MA 02142.},
