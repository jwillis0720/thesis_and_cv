file = {{Proc Natl Acad Sci USA 2013 Schmidt.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2013/Schmidt/Proc Natl Acad Sci USA 2013 Schmidt.pdf:application/pdf;Proc Natl Acad Sci USA 2013 Schmidt.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2013/Schmidt/Proc Natl Acad Sci USA 2013 Schmidt.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1073/pnas.1218256109}}
}

@article{Julien:2012dg,
author = {Julien, Jean-Philippe and Lee, Peter S and Wilson, Ian A},
title = {{Structural insights into key sites of vulnerability on HIV-1 Env and influenza HA.}},
