date-added = {2012-10-22T18:51:52GMT},
date-modified = {2014-03-17T02:36:48GMT},
abstract = {3DLigandSite is a web server for the prediction of ligand-binding sites. It is based upon successful manual methods used in the eighth round of the Critical Assessment of techniques for protein Structure Prediction (CASP8). 3DLigandSite utilizes protein-structure prediction to provide structural models for proteins that have not been solved. Ligands bound to structures similar to the query are superimposed onto the model and used to predict the binding site. In benchmarking against the CASP8 targets 3DLigandSite obtains a Matthew's correlation co-efficient (MCC) of 0.64, and coverage and accuracy of 71 and 60%, respectively, similar results to our manual performance in CASP8. In further benchmarking using a large set of protein structures, 3DLigandSite obtains an MCC of 0.68. The web server enables users to submit either a query sequence or structure. Predictions are visually displayed via an interactive Jmol applet. 3DLigandSite is available for use at http://www.sbg.bio.ic.ac.uk/3dligandsite.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20513649&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2010/Wass/Nucleic%20Acids%20Res%202010%20Wass.pdf},
file = {{Nucleic Acids Res 2010 Wass.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2010/Wass/Nucleic Acids Res 2010 Wass.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1093/nar/gkq406}}
}

@article{Casali:2006dn,
author = {Casali, Paolo and Pal, Zsuzsanna and Xu, Zhenming and Zan, Hong},
title = {{DNA repair in antibody somatic hypermutation.}},
journal = {Trends in immunology},
year = {2006},
volume = {27},
number = {7},
pages = {313--321},
month = jul,
affiliation = {Center for Immunology, School of Medicine and School of Biological Sciences, University of California, Irvine, CA 92697-4120, USA. pcasali@uci.edu},
doi = {10.1016/j.it.2006.05.001},
pmid = {16737852},
language = {English},
rating = {0},
date-added = {2014-03-12T22:29:30GMT},
date-modified = {2014-03-17T02:36:48GMT},
abstract = {Somatic hypermutation (SHM) underlies the generation of a diverse repertoire of high-affinity antibodies. It is effected by a two-step process: (i) DNA lesions initiated by activation-induced cytidine deaminase (AID), and (ii) lesion repair by the combined intervention of DNA replication and repair factors that include mismatch repair (MMR) proteins and translesion DNA synthesis (TLS) polymerases. AID and TLS polymerases that are crucial to SHM, namely polymerase (pol) theta, pol zeta and pol eta, are induced in B cells by the stimuli that are required to trigger this process: B-cell receptor crosslinking and CD40 engagement by CD154. These polymerases, together with MMR proteins and other DNA replication and repair factors, could assemble to form a multimolecular complex ("mutasome") at the site of DNA lesions. Molecular interactions in the mutasome would result in a "polymerase switch", that is, the substitution of the high-fidelity replicative pol delta and pol epsilon with the TLS pol theta, pol eta, Rev1, pol zeta and, perhaps, pol iota, which are error-prone and crucially insert mismatches or mutations while repairing DNA lesions. Here, we place these concepts in the context of the existing in vivo and in vitro findings, and discuss an integrated mechanistic model of SHM.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=16737852&retmode=ref&cmd=prlinks},
uri = {\url{papers2://publication/doi/10.1016/j.it.2006.05.001}}
}

