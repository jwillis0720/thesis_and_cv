number = {26},
pages = {9029--9034},
month = jul,
annote = {Affinity maturation selected from phage display libraries that are guided by computational selection. },
affiliation = {Protein Technology Unit, Biotechnology Programme, Spanish National Cancer Center CNIO, 28029 Madrid, Spain.},
