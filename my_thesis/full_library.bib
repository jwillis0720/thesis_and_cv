@article{Georgiou:2014fi,
author = {Georgiou, George and Georgiou, George and Ippolito, Gregory C and Ippolito, Gregory C and Beausang, John and Beausang, John and Busse, Christian E and Busse, Christian E and Wardemann, Hedda and Wardemann, Hedda and Quake, Stephen R and Quake, Stephen R},
title = {{The promise and challenge of high-throughput sequencing of the antibody repertoire.}},
journal = {Nature biotechnology},
year = {2014},
volume = {32},
number = {2},
pages = {158--168},
month = jan,
affiliation = {1] Department of Chemical Engineering, University of Texas at Austin, Austin, Texas, USA. [2] Department of Biomedical Engineering, University of Texas at Austin, Austin, Texas, USA. [3] Department of Molecular Biosciences, University of Texas at Austin, Austin, Texas, USA. [4] Institute for Cell and Molecular Biology, University of Texas at Austin, Austin, Texas, USA.},
doi = {10.1038/nbt.2782},
pmid = {24441474},
language = {English},
read = {Yes},
rating = {5},
date-added = {2014-02-20T00:16:31GMT},
date-modified = {2014-03-17T02:39:19GMT},
abstract = {Efforts to determine the antibody repertoire encoded by B cells in the blood or lymphoid organs using high-throughput DNA sequencing technologies have been advancing at an extremely rapid pace and are transforming our understanding of humoral immune responses. Information gained from high-throughput DNA sequencing of immunoglobulin genes (Ig-seq) can be applied to detect B-cell malignancies with high sensitivity, to discover antibodies specific for antigens of interest, to guide vaccine development and to understand autoimmunity. Rapid progress in the development of experimental protocols and informatics analysis tools is helping to reduce sequencing artifacts, to achieve more precise quantification of clonal diversity and to extract the most pertinent biological information. That said, broader application of Ig-seq, especially in clinical settings, will require the development of a standardized experimental design framework that will enable the sharing and meta-analysis of sequencing data generated by different laboratories.},
url = {http://www.nature.com/doifinder/10.1038/nbt.2782},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2014/Georgiou/Nat.%20Biotechnol.%202014%20Georgiou-2.pdf},
file = {{Nat. Biotechnol. 2014 Georgiou-2.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2014/Georgiou/Nat. Biotechnol. 2014 Georgiou-2.pdf:application/pdf;Nat. Biotechnol. 2014 Georgiou-2.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2014/Georgiou/Nat. Biotechnol. 2014 Georgiou-2.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1038/nbt.2782}}
}

@article{Khayat:2013ff,
