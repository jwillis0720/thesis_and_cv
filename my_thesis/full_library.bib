year = {2009},
volume = {27},
pages = {693--733},
affiliation = {Laboratory of Gene Regulation and Signal Transduction, Departments of Pharmacology and Pathology, Cancer Center, University of California, San Diego, California 93093, USA.},
doi = {10.1146/annurev.immunol.021908.132641},
