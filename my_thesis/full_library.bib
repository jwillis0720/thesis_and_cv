number = {3},
pages = {114--124},
month = mar,
affiliation = {ExSAR Corporation, Monmouth Junction, NJ 08852, USA.},
doi = {10.1002/jmr.1169},
pmid = {22407975},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-10-22T18:09:44GMT},
date-modified = {2014-03-17T02:36:39GMT},
abstract = {Understanding antigen-antibody interactions at the sub-molecular level is of particular interest for scientific, regulatory, and intellectual property reasons, especially with increasing demand for monoclonal antibody therapeutic agents. Although various techniques are available for the determination of an epitope, there is no widely applicable, high-resolution, and reliable method available. Here, a combination approach using amide hydrogen/deuterium exchange coupled with proteolysis and mass spectrometry (HDX-MS) and computational docking was applied to investigate antigen-antibody interactions. HDX-MS is a widely applicable, medium-resolution, medium-throughput technology that can be applied to epitope identification. First, the epitopes of cytochrome c-E8, IL-13-CNTO607, and IL-17A-CAT-2200 interactions identified using the HDX-MS method were compared with those identified by X-ray co-crystal structures. The identified epitopes are in good agreement with those identified using high-resolution X-ray crystallography. Second, the HDX-MS data were used as constraints for computational docking. More specifically, the non-epitope residues of an antigen identified using HDX-MS were designated as binding ineligible during computational docking. This approach, termed HDX-DOCK, gave more tightly clustered docking poses than stand-alone docking for all antigen-antibody interactions examined and improved docking results significantly for the cytochrome c-E8 interaction.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22407975&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2012/Pandit/J%20Mol%20Recognit%202012%20Pandit.pdf},
file = {{J Mol Recognit 2012 Pandit.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2012/Pandit/J Mol Recognit 2012 Pandit.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1002/jmr.1169}}
}

