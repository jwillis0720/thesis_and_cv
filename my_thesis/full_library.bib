year = {2003},
volume = {326},
number = {4},
pages = {1239--1259},
month = feb,
affiliation = {Howard Hughes Medical Institute and Department of Biochemistry, J-567 Health Sciences, Box 357350, University of Washington, Seattle, WA 98195-7350, USA.},
