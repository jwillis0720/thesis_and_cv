@article{Lungu:2012gr,
author = {Lungu, Oana I and Hallett, Ryan A and Choi, Eun Jung and Aiken, Mary J and Hahn, Klaus M and Kuhlman, Brian},
title = {{Designing photoswitchable peptides using the AsLOV2 domain.}},
journal = {Chemistry {\&} biology},
year = {2012},
volume = {19},
number = {4},
pages = {507--517},
month = apr,
affiliation = {Department of Biochemistry and Biophysics, University of North Carolina, Chapel Hill, NC 27599, USA.},
