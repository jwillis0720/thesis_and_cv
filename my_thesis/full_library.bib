@article{Hu:2006dp,
author = {Hu, Xiaozhen and Kuhlman, Brian},
title = {{Protein design simulations suggest that side-chain conformational entropy is not a strong determinant of amino acid environmental preferences.}},
journal = {Proteins},
year = {2006},
volume = {62},
number = {3},
pages = {739--748},
month = mar,
affiliation = {Department of Biochemistry and Biophysics, University of North Carolina, Chapel Hill 27599, USA.},
doi = {10.1002/prot.20786},
pmid = {16317667},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-03-26T03:50:04GMT},
date-modified = {2014-03-17T02:36:33GMT},
abstract = {Loss of side-chain conformational entropy is an important force opposing protein folding and the relative preferences of the amino acids for being buried or solvent exposed may be partially determined by which amino acids lose more side-chain entropy when placed in the core of a protein. To investigate these preferences, we have incorporated explicit modeling of side-chain entropy into the protein design algorithm, RosettaDesign. In the standard version of the program, the energy of a particular sequence for a fixed backbone depends only on the lowest energy side-chain conformations that can be identified for that sequence. In the new model, the free energy of a single amino acid sequence is calculated by evaluating the average energy and entropy of an ensemble of structures generated by Monte Carlo sampling of amino acid side-chain conformations. To evaluate the impact of including explicit side-chain entropy, sequences were designed for 110 native protein backbones with and without the entropy model. In general, the differences between the two sets of sequences are modest, with the largest changes being observed for the longer amino acids: methionine and arginine. Overall, the identity between the designed sequences and the native sequences does not increase with the addition of entropy, unlike what is observed when other key terms are added to the model (hydrogen bonding, Lennard-Jones energies, and solvation energies). These results suggest that side-chain conformational entropy has a relatively small role in determining the preferred amino acid at each residue position in a protein.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=16317667&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2006/Hu/Proteins%202006%20Hu.pdf},
file = {{Proteins 2006 Hu.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2006/Hu/Proteins 2006 Hu.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1002/prot.20786}}
}

@article{Ambroggio:2006he,
author = {Ambroggio, Xavier I and Kuhlman, Brian},
title = {{Computational design of a single amino acid sequence that can switch between two distinct protein folds.}},
journal = {Journal of the American Chemical Society},
year = {2006},
volume = {128},
number = {4},
pages = {1154--1161},
month = feb,
affiliation = {Department of Biochemistry and Biophysics, University of North Carolina, Chapel Hill, 27599, USA.},
doi = {10.1021/ja054718w},
pmid = {16433531},
language = {English},
rating = {0},
date-added = {2013-03-26T03:50:33GMT},
date-modified = {2014-03-17T02:36:34GMT},
abstract = {The functions of many proteins are mediated by specific conformational changes, and therefore the ability to design primary sequences capable of secondary and tertiary changes is an important step toward the creation of novel functional proteins. To this end, we have developed an algorithm that can optimize a single amino acid sequence for multiple target structures. The algorithm consists of an outer loop, in which sequence space is sampled by a Monte Carlo search with simulated annealing, and an inner loop, in which the effect of a given mutation is evaluated on the various target structures by using the rotamer packing routine and composite energy function of the protein design software, RosettaDesign. We have experimentally tested the method by designing a peptide, Sw2, which can be switched from a 2Cys-2His zinc finger-like fold to a trimeric coiled-coil fold, depending upon the pH or the presence of transition metals. Physical characterization of Sw2 confirms that it is able to reversibly adopt each intended target fold.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=16433531&retmode=ref&cmd=prlinks},
uri = {\url{papers2://publication/doi/10.1021/ja054718w}}
}

@article{Butterfoss:2006jf,
author = {Butterfoss, Glenn L and Kuhlman, Brian},
