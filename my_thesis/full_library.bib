journal = {Structure (London, England : 1993)},
year = {2008},
volume = {16},
number = {2},
pages = {181--195},
month = feb,
affiliation = {Department of Chemistry, Center for Structural Biology, Vanderbilt University, Nashville, TN 37212, USA.},
doi = {10.1016/j.str.2007.11.015},
