uri = {\url{papers2://publication/uuid/CAA0D8C2-C7B0-4FD9-A5B0-13F12A9A4C66}}
}

@article{Bhat:1994wy,
author = {Bhat, T N and Bentley, G A and Boulot, G and Greene, M I and Tello, D and Dall'Acqua, W and Souchon, H and Schwarz, F P and Mariuzza, R A and Poljak, R J},
title = {{Bound water molecules and conformational stabilization help mediate an antigen-antibody association}},
journal = {The king cobra genome reveals dynamic gene evolution and adaptation in the snake venom system},
year = {1994},
volume = {91},
number = {3},
pages = {1089--1093},
month = feb,
annote = {This shows evidence that even though you get negative entropic effects by not releasing the waters from solvent pocket, the hydrogen bonds that mediate binding between solvent and complex contribute largely to an enthalpic effect which drives the complex formation. Largely, you will not get binding if both the waters are not removed and you don't get hydrogen bonds formed. This is called the entropic (solvent) effect and is favored for when you drive waters out of a hydrophobic pocket. Burying of hydrophobic residues is entropically driven. Forming hydrogen bonds is enthalpically driven. },
affiliation = {Centre National de la Recherche Scientifique, Unit{\'e} Associ{\'e}e 359, Institut Pasteur, Paris, France.},
keywords = {PNAS_Paper},
pmid = {8302837},
language = {eng},
