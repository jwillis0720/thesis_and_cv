volume = {276},
number = {5319},
pages = {1665--1669},
month = jun,
annote = {this paper has everything mapped to atomci detail. The preorganization of the binding site elimitates entropic cost by 30,000. Distal mutations help out. A lot of mutations are also at the in terface},
