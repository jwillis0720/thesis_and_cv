title = {{Contrasting IgG structures reveal extreme asymmetry and flexibility}},
journal = {Journal of molecular biology},
year = {2002},
volume = {319},
number = {1},
pages = {9--18},
month = may,
annote = {Rotation and flexibility at the hinge region play a critical role to binding by orienting the Fv region to the epitope.},
affiliation = {Department of Molecular Biology, The Scripps Research Institute, 10550 North Torrey Pines Road, La Jolla, CA 92037, USA.},
keywords = {PNAS_Paper},
