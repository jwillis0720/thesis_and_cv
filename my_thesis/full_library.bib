title = {{Residue-Level Prediction of HIV-1 Antibody Epitopes Based on Neutralization of Diverse Viral Strains.}},
journal = {Journal Of Virology},
year = {2013},
month = jul,
affiliation = {Vaccine Research Center, National Institute of Allergy and Infectious Diseases, National Institutes of Health, Bethesda, Maryland 20892, USA.},
doi = {10.1128/JVI.00984-13},
pmid = {23843642},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-07-30T03:31:13GMT},
date-modified = {2014-03-17T02:36:38GMT},
abstract = {Delineation of antibody epitopes at the residue level is key to understanding antigen resistance mutations, designing epitope-specific probes for antibody isolation, and developing epitope-based vaccines. Ideally, epitope residues are determined in the context of atomic-level structure of the antibody-antigen complex, though structure determination may in many cases be impractical. Here we describe an efficient computational method to predict antibody-specific HIV-1 envelope (Env) epitopes at the residue level, based on neutralization panels of diverse viral strains. The method primarily utilizes neutralization potency data over a set of diverse viral strains representing the antigen, and enhanced accuracy could be achieved by incorporating information from the unbound structure of the antigen. The method was evaluated on 19 HIV-1 Env antibodies with neutralization panels comprising 181 diverse viral strains and with available antibody-antigen complex structures. Prediction accuracy was shown to significantly improve over random selection with an average of greater than 8-fold enrichment of true positives at the 0.05 false positive rate level. The method was used to prospectively predict epitope residues for two HIV-1 antibodies, 8ANC131 and 8ANC195, for which predictions were validated experimentally. The method is inherently applicable to antigens that exhibit sequence diversity, and its accuracy was found to correlate inversely with sequence conservation of the epitope. Together the results show how knowledge inherent to a neutralization panel and unbound antigen structure can be utilized for residue-level prediction of antibody epitopes.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=23843642&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2013/Chuang/J%20Virol%202013%20Chuang.pdf},
file = {{J Virol 2013 Chuang.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2013/Chuang/J Virol 2013 Chuang.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1128/JVI.00984-13}}
}

@article{DiMaio:2011fs,
