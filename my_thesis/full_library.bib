uri = {\url{papers2://publication/doi/10.1016/j.str.2004.12.004}}
}

@article{Chen:2005bo,
author = {Chen, Bing and Vogan, Erik M and Gong, Haiyun and Skehel, John J and Wiley, Don C and Harrison, Stephen C},
title = {{Structure of an unliganded simian immunodeficiency virus gp120 core}},
journal = {Nature},
year = {2005},
volume = {433},
number = {7028},
pages = {834--841},
