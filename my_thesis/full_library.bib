}

@article{Nauli:2002bk,
author = {Nauli, Sehat and Kuhlman, Brian and Le Trong, Isolde and Stenkamp, Ronald E and Teller, David and Baker, David},
title = {{Crystal structures and increased stabilization of the protein G variants with switched folding pathways NuG1 and NuG2.}},
