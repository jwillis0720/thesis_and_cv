@article{Weitkamp:2003tp,
author = {Weitkamp, J{\"o}rn-Hendrik and Kallewaard, Nicole and Kusuhara, Koichi and Bures, Elizabeth and Williams, John V and LaFleur, Bonnie and Greenberg, Harry B and Crowe, James E},
title = {{Infant and adult human B cell responses to rotavirus share common immunodominant variable gene repertoires.}},
journal = {Journal of immunology (Baltimore, Md : 1950)},
year = {2003},
volume = {171},
number = {9},
pages = {4680--4688},
month = nov,
affiliation = {Department of Pediatrics, Vanderbilt University Medical Center, Nashville, TN 37232-2581, USA.},
pmid = {14568943},
language = {English},
read = {Yes},
rating = {0},
date-added = {2010-02-04T17:43:19GMT},
date-modified = {2014-03-17T02:35:46GMT},
abstract = {Ab repertoires exhibit marked restrictions during fetal life characterized by biases of variable gene usage and lack of junctional diversity. We tested the hypothesis that Ab repertoire restriction contributes to the observed poor quality of specific Ab responses made by infants to viral infections. We analyzed the molecular determinants of B cell responses in humans to two Ags of rotavirus (RV), a common and clinically important infection of human infants. We sequenced Ab H and L chain V region genes (V(H) and V(L)) of clones expanded from single B cells responding to RV virus protein 6 or virus protein 7. We found that adults exhibited a distinct bias in use of gene segments in the V(H)1 and V(H)4 families, for example, V(H)1-46, V(H)4-31, and V(H)4-61. This gene segment bias differed markedly from the V(H)3 dominant bias seen in randomly selected adult B cells. Recombinant Abs incorporating any of those three immunodominant V(H) segments bound to RV-infected cells and also to purified RV particles. The RV-specific B cell repertoires of infants aged 2-11 mo and those of adults were highly related when compared by V(H), D, J(H), V(L), and J(L) segment selection, extent of junctional diversity, and mean H chain complementarity determining region 3 length. These data suggest that residual fetal bias of the B cell repertoire is not a limiting determinant of the quality of Ab responses to viruses of infants beyond the neonatal period.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=14568943&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2003/Weitkamp/J%20Immunol%202003%20Weitkamp.pdf},
file = {{J Immunol 2003 Weitkamp.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2003/Weitkamp/J Immunol 2003 Weitkamp.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/54F0937A-5B2A-43B2-B057-4516AEFA7691}}
}

@article{Crooks:2004do,
author = {Crooks, Gavin E and Hon, Gary and Chandonia, John-Marc and Brenner, Steven E},
