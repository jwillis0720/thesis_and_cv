uri = {\url{papers2://publication/doi/10.1021/ja106173n}}
}

@article{Pandit:2012gk,
author = {Pandit, Deepangi and Tuske, Steven J and Coales, Stephen J and E, Sook Yen and Liu, Anita and Lee, Jessica E and Morrow, Jeffrey A and Nemeth, Jennifer F and Hamuro, Yoshitomo},
title = {{Mapping of discontinuous conformational epitopes by amide hydrogen/deuterium exchange mass spectrometry and computational docking.}},
