@article{Wass:2010er,
author = {Wass, Mark N and Kelley, Lawrence A and Sternberg, Michael J E},
title = {{3DLigandSite: predicting ligand-binding sites using similar structures.}},
journal = {Nucleic acids research},
year = {2010},
