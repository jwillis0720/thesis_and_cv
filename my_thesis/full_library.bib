journal = {Immunity},
year = {2006},
volume = {24},
number = {4},
pages = {429--438},
month = apr,
affiliation = {National Institute of Immunology, New Delhi 110067, India.},
keywords = {PNAS_Paper, tag},
doi = {10.1016/j.immuni.2006.02.010},
