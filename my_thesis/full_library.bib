uri = {\url{papers2://publication/doi/10.1111/j.1747-0285.2012.01356.x}}
}

@article{Kaufmann:2009cq,
author = {Kaufmann, Kristian W and Dawson, Eric S and Henry, L Keith and Field, Julie R and Blakely, Randy D and Meiler, Jens},
