@article{Angelova:2010ka,
author = {Angelova, Krassimira and de Jonge, Hugo and Granneman, Joke C M and Puett, David and Bogerd, Jan},
title = {{Functional differences of invariant and highly conserved residues in the extracellular domain of the glycoprotein hormone receptors.}},
journal = {The Journal of biological chemistry},
year = {2010},
volume = {285},
number = {45},
pages = {34813--34827},
month = nov,
affiliation = {Department of Biochemistry and Molecular Biology, University of Georgia, Athens, Georgia 30602, USA.},
