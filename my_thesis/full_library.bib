uri = {\url{papers2://publication/uuid/F102A2F7-3BC6-4F5E-82E8-9DDD21ED98B6}}
}

@article{Stiegler:2001ew,
author = {Stiegler, G and Kunert, R and Purtscher, M and Wolbank, S and Voglauer, R and Steindl, F and Katinger, H},
