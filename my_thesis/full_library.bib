file = {{J Virol 2011 Lynch.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2011/Lynch/J Virol 2011 Lynch.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1128/JVI.02006-10}}
}

