language = {eng},
read = {Yes},
rating = {0},
date-added = {2009-10-09T23:47:08GMT},
date-modified = {2014-03-17T02:38:29GMT},
abstract = {The first step in HIV infection is the binding of the envelope glycoprotein gp120 to the host cell receptor CD4. An interfacial "Phe43 cavity" in gp120, adjacent to residue Phe43 of gp120-bound CD4, has been suggested as a potential target for therapeutic intervention. We designed a CD4 mutant (D1D2F43C) for site-specific coupling of compounds for screening against the cavity. Altogether, 81 cysteine-reactive compounds were designed, synthesized, and tested. Eight derivatives exceeded the affinity of native D1D2 for gp120. Structure-activity relationships (SAR) for derivatized CD4 binding to gp120 revealed significant plasticity of the Phe43 cavity and a narrow entrance. The primary contacts for compound recognition inside the cavity were found to be van der Waals interactions, whereas hydrophilic interactions were detected in the entrance. This first SAR on ligand binding to an interior cavity of gp120 may provide a starting point for structure-based assembly of small molecules targeting gp120-CD4 interaction.},
url = {http://pubs.acs.org/doi/abs/10.1021/jm070564e},
local-url = {file://localhost/Users/jordanwillis/Dropbox/Papers2/Articles/2007/Xie/2007%20Xie.pdf},
