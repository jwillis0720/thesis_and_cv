file = {{J Immunol 2008 Tian.pdf:/Users/jordanwillis/Dropbox/Papers2/Articles/2008/Tian/J Immunol 2008 Tian.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/ED7713F2-9CB7-4495-92C9-6027CC9678E1}}
}

@article{Scamurra:2000uk,
