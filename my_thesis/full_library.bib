journal = {Biophysical journal},
year = {1997},
volume = {73},
number = {4},
pages = {2116--2125},
month = oct,
annote = {Gives actual entropic measurements of how entropy decreseases on bidnign but how you can liberate water. This is a great study showing how their are more than just rotational degreees of freedom. Their is also water moving in and out of the interface upon complex formation. },
affiliation = {Department of Chemical Engineering, University of Houston, Texas 77204-4792, USA.},
keywords = {PNAS_Paper},
